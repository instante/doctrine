{
    "name": "instante/doctrine",
    "type": "library",
    "description": "Improved wrapper of Kdyby\\Doctrine integration into Nette",
    "keywords": [
        "nette",
        "instante"
    ],
    "license": [
        "BSD-3-Clause",
        "GPL-2.0",
        "GPL-3.0"
    ],
    "require": {
        "nette/di": "~2.2",
        "nette/security": "~2.2",

        "doctrine/migrations": "@dev",
        "kdyby/doctrine": "@dev",
        "ircmaxell/password-compat" : "~1.0"
    },
    "require-dev": {
        "nette/tester": "@dev",
        "nette/http": "~2.2",
        "jakub-onderka/php-parallel-lint": "~0.6",
        "doctrine/data-fixtures": "~1.2"
    },
    "suggest": {
        "doctrine/data-fixtures": "to use Doctrine Data Fixtures API"
    },
    "autoload": {
        "psr-0": {
            "Instante\\Doctrine\\": "src/"
        }
    }
}
